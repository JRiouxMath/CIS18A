public static void main (String[], args)
{
  double a;
  int b;
  fastExp(a, b);
  {  if (b = 0)
    {
      result = 1;
    }//End if b = 0
    if (b = 1)
    {
      result = a;
    }// End if b = 1
    while (b>=0)
    {
      if (b%2 = 1)
       {
          result *=a;
       }// End if  
          b/=2;
